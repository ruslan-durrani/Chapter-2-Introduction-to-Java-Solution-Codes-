import java.util.Scanner;
public class Practice {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        System.out.print("Number: ");
        int num = sc.nextInt();
        int x = 0 ;
        int y = 1;
        for (int i = 1; i <= num; i++) {
            System.out.print(x+" ");
            System.out.print(y+" ");
            x = y + x ;
            y += x;
        }
    }
}
